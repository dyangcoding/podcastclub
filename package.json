{
  "name": "podcast-client",
  "version": "1.0.0",
  "engines": {
    "node": "10.16.0"
  },
  "description": "podcast client that consumes data from podcast backend",
  "author": "tryToCode",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "heroku-postbuild": "npm run build",
    "test:e2e": "node test/e2e/nightwatch.js",
    "test": "ava --verbose",
    "test:watch": "ava --watch"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/unit/setup.js"
    ],
    "files": [
      "./test/unit/specs/*.js"
    ],
    "snapshotDir": "./test/unit/snapshots"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.8.0",
    "@nuxtjs/google-analytics": "^2.2.1",
    "@nuxtjs/moment": "^1.4.0",
    "@nuxtjs/tailwindcss": "^1.2.0",
    "nuxt": "^2.10.2",
    "serialize-javascript": "^2.1.1",
    "vue-json-pretty": "^1.6.2",
    "vue-spinner": "^1.0.3",
    "vue-tippy": "^4.0.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.30",
    "ava": "^2.4.0",
    "babel-register": "^6.26.0",
    "browser-env": "^3.3.0",
    "geckodriver": "^1.19.1",
    "nightwatch": "^1.3.2",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "sinon": "^7.5.0"
  }
}
